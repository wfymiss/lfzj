<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shape_update"
    android:orientation="vertical">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@mipmap/health_dialog2" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="38dp"
        android:text="请准时到达" />

    <TextView
        android:id="@+id/tv_ok"
        android:layout_width="201dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="17dp"
        android:background="@drawable/shape_login_text"
        android:gravity="center"
        android:text="完成"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/updata_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_14dp"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/blu"
        android:layout_marginBottom="@dimen/dp_40"
        android:text="更改预约时间" />


</LinearLayout>